    },
    "1659349393172e187e": {
      "type": "rich-text",
      "blocks": {
        "1659349393f78d2d47-0": {
          "type": "heading",
          "settings": {
            "title": "Assured Quality:"
          }
        },
        "1659349393f78d2d47-1": {
          "type": "text",
          "settings": {
            "enlarge_text": false,
            "text": "<p>Guarantee 100% authenticity of all products on our platform, curating key pieces for each season to ensure customers always have access to the latest and best in ethnic Indian fashion.<\/p>"
          }
        }
      },
