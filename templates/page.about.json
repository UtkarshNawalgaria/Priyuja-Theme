    "1659348964d401dad3": {
      "type": "text-with-icons",
      "blocks": {
        "1659348964c874d33e-0": {
          "type": "text_block",
          "settings": {
            "icon": "globe",
            "title": "Our Mission",
            "text": "<p>Our goal is to help manufacturers and designers in Kolkata sell their products directly to customers, bypassing intermediaries and offering genuine products at an affordable price. At Priyuja, we continue Albeli's legacy by discovering and supporting emerging talent in the fashion industry.<\/p>"
          }
        },
        "f2bfeed3-8a7a-4460-b06b-80db97a8ec5c": {
          "type": "text_block",
          "settings": {
            "icon": "trophy",
            "title": "Quality Assurance",
            "text": "<p>Guarantee 100% authenticity of all products on our platform, curating key pieces for each season to ensure customers always have access to the latest and best in ethnic Indian fashion.<\/p>"
          }
        }
      },
